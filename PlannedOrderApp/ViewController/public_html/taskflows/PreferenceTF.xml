<?xml version="1.0" encoding="windows-1252" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
  <task-flow-definition id="PreferenceTF">
    <default-activity>Preference</default-activity>
    <managed-bean id="__1">
      <managed-bean-name>PreferenceBean</managed-bean-name>
      <managed-bean-class>deloitte.retail.mobile.beans.HomeBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <view id="Preference">
      <page>/pages/Preference.amx</page>
    </view>
    <page-flow-scope-behavior>
      <push-new/>
    </page-flow-scope-behavior>
  </task-flow-definition>
</adfc-mobile-config>
